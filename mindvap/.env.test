# Test Environment Configuration for MindVap
# This file enables mock authentication for local testing

# Enable mock authentication (bypasses email verification)
SKIP_EMAIL_VERIFICATION=true

# Development mode
NODE_ENV=development

# Mock authentication settings
MOCK_AUTH_ENABLED=true

# Debug mode
DEBUG=true

# Note: For production, you would use real Supabase credentials
# VITE_SUPABASE_URL=your_supabase_url
# VITE_SUPABASE_ANON_KEY=your_supabase_key